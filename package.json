{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "main": "server.js",
  "scripts": {
    "bash": "docker-compose exec server bash",
    "node": "docker-compose exec server node",
    "psql": "heroku pg:psql -a signs-uploader-api",
    "ps": "heroku ps -a signs-uploader-api",
    "dev:chrome": "open -a \"Google Chrome\" https://local.auth:4000",
    "dev": "docker-compose down && docker-compose up",
    "dev:stop": "docker-compose down --remove-orphans",
    "dev:rebuild": "docker-compose down --remove-orphans && docker-compose up --build",
    "es:reindex": "docker-compose exec server node scripts/reindexElasticsearchForSigns.js",
    "db:generate": "npx sequelize-cli migration:generate ---name $NAME ---attributes $ATTRIBUTES",
    "db:migrate": "npx sequelize db:migrate",
    "db:seed": "npx sequelize db:seed",
    "start:dev": "nodemon server.js",
    "release:migrate": "npx sequelize db:migrate",
    "release:reindex": "node scripts/reindexElasticsearchForSigns.js",
    "release": "npm run release:migrate && npm run release:reindex",
    "lint": "eslint .",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --coverage",
    "seed:user": "docker-compose exec server node scripts/connectAuthenticationUserToAppUser.js $1 $2 $3"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "7.13.0",
    "apollo-server-express": "^2.22.2",
    "auth0": "^2.34.0",
    "aws-sdk": "^2.868.0",
    "connect-session-firebase": "^5.13.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "express-jwt": "^6.0.0",
    "express-jwt-authz": "^2.4.1",
    "express-session": "^1.17.1",
    "firebase": "^8.3.1",
    "firebase-admin": "^9.5.0",
    "graphql": "^15.5.0",
    "graphql-auth-user-directives": "^2.4.4",
    "graphql-tools": "^7.0.4",
    "graphql-type-uuid": "^0.2.0",
    "graphql-upload": "^11.0.0",
    "jwks-rsa": "^2.0.1",
    "lodash": "^4.17.21",
    "pg": "^8.5.1",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "uuid": "^8.3.2",
    "winston": "^3.6.0",
    "xstate": "^4.17.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "eslint": "^7.25.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.7",
    "sequelize-mock": "^0.10.2",
    "supertest": "^6.2.2"
  },
  "engines": {
    "node": "12.x"
  }
}

    "pg": "^8.5.1",
    "sequelize": "^6.6.2",
    "uuid": "^8.3.2",
    "xstate": "^4.17.0"
  },
  "devDependencies": {
    "eslint": "^7.25.0",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0"
  },
  "engines": {
    "node": "12.x"
  }
